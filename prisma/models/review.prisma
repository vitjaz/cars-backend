model Review {
  id Int @id @default(autoincrement())
  title String
  desc String
  photoUrls String[]
  rating Int  // 1-5
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  user User @relation(fields: [userId], references: [id])
  userId Int

  vehicle Vehicle @relation(fields: [vehicleId], references: [id])
  vehicleId Int

  @@map("reviews")
}