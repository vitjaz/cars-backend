// Объявление (advertisment)

model AD {
  id Int @id @default(autoincrement())
  title String
  user User @relation(fields: [userId], references: [id])
  userId Int
  viewCount Int
  price Int
  desc String
  photoUrls String[]
  nationalNumber String
  year Int
  mileage Int
  vin String? @unique

  vehicle  Vehicle @relation(fields: [vehicleId], references: [id])
  vehicleId   Int

  @@map("ads")
}